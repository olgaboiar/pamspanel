<tr id="student-<%= student.id %>">
    <td><%= student.id %></td>
    <td><%= student.first_name %></td>
    <td><%= student.last_name %></td>
    <td><%= student.email %></td>
    <td><%= student.phone %></td>
    <td><%= student.birthday %></td>
    <td><%= ((Time.zone.now - student.birthday.to_time) / 1.year.seconds).floor %></td>
    <% if student.cohort_id == nil %>
    <td>Not assigned</td><% else %>
    <td><%= student.cohort.name %></td><% end %>
    <td><%= student.education %></td>
    <td><%= link_to 'Edit', edit_student_path(student), remote:true, class: "modal-trigger"%></td>
    <td><%= link_to 'Delete', student_path(student), remote:true, method: :delete, data: { confirm: 'Are you sure?' } %></td>
</tr>