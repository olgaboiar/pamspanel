<div class="container">
<h5>Fill in this form to create a new student:</h5>
<%= form_for @student do |f| %>  
    <%= f.label :first_name %>  
    <%= f.text_field :first_name, :required => true %>  
    <%= f.label :last_name %>  
    <%= f.text_field :last_name, :required => true %>
    <%= f.label :email %>  
    <%= f.text_field :email %>
    <%= f.label :phone %> 
    <%= f.text_field :phone %>
    
    <%= f.label :birthday, :required => true %> 
    <%= f.text_field :birthday %>
    <%= f.label :education %> 
    <%= f.select :education, ['HS', 'College', 'Masters', 'PhD'] %> 
    <button class="btn waves-effect waves-light grey" type="submit" id="form-main-button"><%= f.submit %></button>
<% end %>
</div>