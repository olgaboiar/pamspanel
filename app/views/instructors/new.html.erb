<div class="container">
<h5>Fill in this form to create a new instructor:</h5>
<%= form_for @instructor do |f| %>  
    <%= f.label :first_name %>  
    <%= f.text_field :first_name %>  
    <%= f.label :last_name %>  
    <%= f.text_field :last_name %>
    <%= f.label :email %>  
    <%= f.text_field :email %>
    <%= f.label :phone %> 
    <%= f.text_field :phone %>
    <%= f.label :user_id %> 
    <%= f.number_field :user_id %>
    <%= f.label :userpic %> 
    <%= f.text_field :userpic %> 
    <%= f.label :birthday %> 
    <%= f.text_field :birthday %>
    <%= f.label :salary %> 
    <%= f.number_field :salary %>
    <%= f.label :education %> 
    <%= f.text_field :education %>  
    <button class="btn waves-effect waves-light grey" type="submit" id="form-main-button"><%= f.submit %></button>
<% end %>
</div>