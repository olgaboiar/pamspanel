<%= form_for @user do |f| %>  
    <%= f.label :username %>  
    <%= f.text_field :username %>  
    <%= f.label :email %>  
    <%= f.text_field :email %>
    <%= f.label :password %>  
    <%= f.text_field :password %>
    <%= f.label :role %> 
    <%= f.text_field :role %>
    <%= f.label :userpic %> 
    <%= f.text_field :userpic %> 
    <%= f.submit %>
<% end %>