<div class="container">
<h5>Fill in this form to create a new cohort:</h5>
<%= form_for @cohort do |f| %>  
    <%= f.label :name %>  
    <%= f.text_field :name %>  
    <%= f.label :class_type %>  
    <%= f.text_field :class_type %>
    <%= f.label :schedule %>  
    <%= f.text_field :schedule %>
    <%= f.label :classroom %> 
    <%= f.text_field :classroom %>
    <%= f.label :course_id %> 
    <%= f.number_field :course_id %>
    <%= f.label :start_date %> 
    <%= f.text_field :start_date %> 
    <%= f.label :end_date %> 
    <%= f.text_field :end_date %>
    <%= f.label :instructor_id %> 
    <%= f.number_field :instructor_id %>
    <%= f.label :picture %> 
    <%= f.text_field :picture %>
    <button class="btn waves-effect waves-light grey" type="submit" id="form-main-button"><%= f.submit %></button>
<% end %>
</div>
